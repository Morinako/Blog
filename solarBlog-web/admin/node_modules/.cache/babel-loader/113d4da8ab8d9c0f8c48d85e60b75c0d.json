CAAA;AACA,WAAA,QAAA;AACA,KAVA;AAWA,IAAA,UAXA,sBAWA,IAXA,EAWA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,WAAA,QAAA;AACA,KAdA;AAeA,IAAA,aAfA,yBAeA,OAfA,EAeA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,WAAA,QAAA;AACA,KAlBA;AAmBA,IAAA,QAnBA,sBAmBA;AAAA;;AACA,UAAA,MAAA,GAAA;AACA,QAAA,OAAA,EAAA,KAAA,OADA;AAEA,QAAA,IAAA,EAAA,KAAA,IAFA;AAGA,QAAA,QAAA,EAAA,KAAA;AAHA,OAAA;AAKA,gCAAA,MAAA,EAAA,IAAA,CAAA,gBAAA;AAAA,YAAA,IAAA,QAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,IAAA,CAAA,IAAA,CAAA,UAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,OALA;AAMA,KA/BA;AAgCA,IAAA,SAhCA,qBAgCA,EAhCA,EAgCA;AAAA;;AACA,UAAA,KAAA,GAAA,EAAA;;AACA,UAAA,EAAA,IAAA,IAAA,EAAA;AACA,QAAA,KAAA,GAAA;AAAA,UAAA,IAAA,EAAA,CAAA,EAAA;AAAA,SAAA;AACA,OAFA,MAEA;AACA,QAAA,KAAA,GAAA;AAAA,UAAA,IAAA,EAAA,KAAA;AAAA,SAAA;AACA;;AACA,WAAA,KAAA,CAAA,MAAA,CAAA,uBAAA,EAAA,KAAA,EAAA,IAAA,CAAA,iBAAA;AAAA,YAAA,IAAA,SAAA,IAAA;;AACA,YAAA,IAAA,CAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,IAAA,CAAA;AAFA,WAAA;;AAIA,UAAA,MAAA,CAAA,QAAA;AACA,SANA,MAMA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,KAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,IAAA,CAAA;AAFA,WAAA;AAIA;;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,KAAA;AACA,OAdA;AAeA,KAtDA;AAuDA,IAAA,KAvDA,iBAuDA,MAvDA,EAuDA;AACA,WAAA,MAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,MAAA,CAAA,CAAA;AACA,WAAA,OAAA,GAAA,IAAA;AACA;AA1DA;AAjBA,C","sourcesContent":["<template>\n  <el-card class=\"main-card\">\n    <!-- 标题 -->\n    <div class=\"title\">{{ this.$route.name }}</div>\n    <div class=\"operation-container\">\n      <el-button\n        type=\"danger\"\n        size=\"small\"\n        icon=\"el-icon-delete\"\n        :disabled=\"this.logIdList.length === 0\"\n        @click=\"isDelete = true\"\n      >\n        批量删除\n      </el-button>\n      <!-- 数据筛选 -->\n      <div style=\"margin-left:auto\">\n        <el-input\n          v-model=\"keywords\"\n          prefix-icon=\"el-icon-search\"\n          size=\"small\"\n          placeholder=\"请输入用户昵称\"\n          style=\"width:200px\"\n          @keyup.enter.native=\"searchLogs\"\n        />\n        <el-button\n          type=\"primary\"\n          size=\"small\"\n          icon=\"el-icon-search\"\n          style=\"margin-left:1rem\"\n          @click=\"searchLogs\"\n        >\n          搜索\n        </el-button>\n      </div>\n    </div>\n    <!-- 登录日志列表 -->\n    <el-table\n      @selection-change=\"selectionChange\"\n      v-loading=\"loading\"\n      :data=\"logList\"\n    >\n      <el-table-column type=\"selection\" align=\"center\" />\n      <el-table-column\n        prop=\"nickname\"\n        label=\"用户昵称\"\n        align=\"center\"\n        width=\"180\"\n      />\n      <el-table-column\n              prop=\"loginType\"\n              label=\"登录方式\"\n              align=\"center\"\n      >\n        <template slot-scope=\"scope\">\n          <el-tag type=\"success\" v-if=\"scope.row.loginType === 1\">邮箱</el-tag>\n          <el-tag v-if=\"scope.row.loginType === 2\">QQ</el-tag>\n          <el-tag type=\"danger\" v-if=\"scope.row.loginType === 3\">微博</el-tag>\n          <el-tag type=\"primary\" v-if=\"scope.row.loginType === 4\">微信</el-tag>\n        </template>\n      </el-table-column>\n      <el-table-column prop=\"ipAddress\" label=\"登录ip\" align=\"center\" />\n      <el-table-column\n              prop=\"ipSource\"\n              label=\"登录地址\"\n              align=\"center\"\n              width=\"90\"\n      />\n      <el-table-column\n              prop=\"browser\"\n              label=\"浏览器\"\n              align=\"center\"\n      />\n      <el-table-column label=\"登录状态\" align=\"center\" prop=\"status\">\n        <template slot-scope=\"scope\">\n          <el-tag type=\"danger\" v-if=\"scope.row.status === 0\">失败</el-tag>\n          <el-tag type=\"success\" v-if=\"scope.row.status === 1\">成功</el-tag>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作信息\" align=\"center\" prop=\"message\" />\n      <el-table-column\n              prop=\"lastLoginTime\"\n              label=\"登录时间\"\n              align=\"center\"\n              width=\"110\"\n      >\n        <template slot-scope=\"scope\">\n          {{ scope.row.createTime | dateTime }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\" align=\"center\">\n        <template slot-scope=\"scope\">\n          <el-popconfirm\n            title=\"确定删除吗？\"\n            style=\"margin-left:10px\"\n            @confirm=\"deleteLog(scope.row.id)\"\n          >\n            <el-button size=\"mini\" type=\"text\" slot=\"reference\">\n              <i class=\"el-icon-delete\" /> 删除\n            </el-button>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n    <!-- 分页 -->\n    <el-pagination\n      class=\"pagination-container\"\n      background\n      @size-change=\"sizeChange\"\n      @current-change=\"currentChange\"\n      :current-page=\"current\"\n      :page-size=\"size\"\n      :total=\"count\"\n      :page-sizes=\"[10, 30,50,100]\"\n      layout=\"total, sizes, prev, pager, next, jumper\"\n    />\n    <!-- 批量删除对话框 -->\n    <el-dialog :visible.sync=\"isDelete\" width=\"30%\" :close-on-click-modal=\"false\">\n      <div class=\"dialog-title-container\" slot=\"title\">\n        <i class=\"el-icon-warning\" style=\"color:#ff9900\" />提示\n      </div>\n      <div style=\"font-size:1rem\">是否删除选中项？</div>\n      <div slot=\"footer\">\n        <el-button @click=\"isDelete = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"deleteLog(null)\">\n          确 定\n        </el-button>\n      </div>\n    </el-dialog>\n  </el-card>\n</template>\n\n<script>\nimport {getLoginLogs} from \"../../api/system\";\n\nexport default {\n  created() {\n    this.listLogs();\n  },\n  data() {\n    return {\n      loading: true,\n      logList: [],\n      logIdList: [],\n      keywords: null,\n      current: 1,\n      size: 10,\n      count: 0,\n      isCheck: false,\n      isDelete: false\n    };\n  },\n  methods: {\n    selectionChange(logList) {\n      this.logIdList = [];\n      logList.forEach(item => {\n        this.logIdList.push(item.id);\n      });\n    },\n    searchLogs() {\n      this.current = 1;\n      this.listLogs();\n    },\n    sizeChange(size) {\n      this.size = size;\n      this.listLogs();\n    },\n    currentChange(current) {\n      this.current = current;\n      this.listLogs();\n    },\n    listLogs() {\n      const params = {\n        current: this.current,\n        size: this.size,\n        keywords: this.keywords,\n      };\n      getLoginLogs(params).then(({ data }) => {\n        console.log(data)\n        this.logList = data.data.recordList;\n        this.count = data.data.count;\n        this.loading = false;\n      });\n    },\n    deleteLog(id) {\n      var param = {};\n      if (id != null) {\n        param = { data: [id] };\n      } else {\n        param = { data: this.logIdList };\n      }\n      this.axios.delete(\"/api/admin/login/logs\", param).then(({ data }) => {\n        if (data.flag) {\n          this.$notify.success({\n            title: \"成功\",\n            message: data.message\n          });\n          this.listLogs();\n        } else {\n          this.$notify.error({\n            title: \"失败\",\n            message: data.message\n          });\n        }\n        this.isDelete = false;\n      });\n    },\n    check(optLog) {\n      this.optLog = JSON.parse(JSON.stringify(optLog));\n      this.isCheck = true;\n    }\n  }\n};\n</script>\n\n<style scoped>\nlabel {\n  font-weight: bold !important;\n}\n</style>\n"],"sourceRoot":"src/views/log"}]}