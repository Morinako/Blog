A,MAMA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,KAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,IAAA,CAAA;AAFA,WAAA;AAIA;;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,KAAA;AACA,OAdA;AAeA,KA7DA;AA8DA,IAAA,UA9DA,sBA8DA,IA9DA,EA8DA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,WAAA,YAAA;AACA,KAjEA;AAkEA,IAAA,aAlEA,yBAkEA,OAlEA,EAkEA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,WAAA,YAAA;AACA,KArEA;AAsEA,IAAA,YAtEA,wBAsEA,MAtEA,EAsEA;AACA,cAAA,MAAA;AACA,aAAA,KAAA;AACA,eAAA,QAAA,GAAA,CAAA;AACA,eAAA,MAAA,GAAA,IAAA;AACA;;AACA,aAAA,QAAA;AACA,eAAA,QAAA,GAAA,CAAA;AACA,eAAA,MAAA,GAAA,CAAA;AACA;;AACA,aAAA,QAAA;AACA,eAAA,QAAA,GAAA,CAAA;AACA,eAAA,MAAA,GAAA,CAAA;AACA;;AACA,aAAA,OAAA;AACA,eAAA,QAAA,GAAA,CAAA;AACA,eAAA,MAAA,GAAA,CAAA;AACA;;AACA,aAAA,QAAA;AACA,eAAA,QAAA,GAAA,CAAA;AACA,eAAA,MAAA,GAAA,IAAA;AACA;AApBA;;AAsBA,WAAA,YAAA,GAAA,MAAA;AACA,KA9FA;AA+FA;AACA,IAAA,SAhGA,qBAgGA,OAhGA,EAgGA;AAAA;;AACA,+BAAA;AAAA,QAAA,EAAA,EAAA,OAAA,CAAA,EAAA;AAAA,QAAA,KAAA,EAAA,OAAA,CAAA;AAAA,OAAA,EAAA,IAAA,CAAA,iBAAA;AAAA,YAAA,IAAA,SAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA,EAAA,IAAA;;AACA,YAAA,IAAA,CAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA;AAFA,WAAA;AAIA,SALA,MAKA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,KAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,IAAA,CAAA;AAFA,WAAA;;AAIA,UAAA,OAAA,CAAA,KAAA,GAAA,OAAA,CAAA,KAAA,KAAA,CAAA,GAAA,CAAA,GAAA,CAAA;AACA;;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,KAAA;AACA,OAfA;AAgBA,KAjHA;AAkHA,IAAA,YAlHA,0BAkHA;AAAA;;AACA,UAAA,MAAA,GAAA;AACA,QAAA,OAAA,EAAA,KAAA,OADA;AAEA,QAAA,IAAA,EAAA,KAAA,IAFA;AAGA,QAAA,QAAA,EAAA,KAAA,QAHA;AAIA,QAAA,UAAA,EAAA,KAAA,UAJA;AAKA,QAAA,MAAA,EAAA,KAAA,MALA;AAMA,QAAA,KAAA,EAAA,KAAA,KANA;AAOA,QAAA,IAAA,EAAA,KAAA,IAPA;AAQA,QAAA,QAAA,EAAA,KAAA;AARA,OAAA;AAUA,mCAAA,MAAA,EAAA,IAAA,CAAA,iBAAA;AAAA,YAAA,IAAA,SAAA,IAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,IAAA,CAAA,IAAA,CAAA,UAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,WAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,OALA;AAMA,KAnIA;AAoIA,IAAA,cApIA,4BAoIA;AAAA;;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,8BAAA,EAAA,IAAA,CAAA,iBAAA;AAAA,YAAA,IAAA,SAAA,IAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,IAAA,CAAA,IAAA;AACA,OAFA;AAGA,KAxIA;AAyIA,IAAA,QAzIA,sBAyIA;AAAA;;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,wBAAA,EAAA,IAAA,CAAA,iBAAA;AAAA,YAAA,IAAA,SAAA,IAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,IAAA,CAAA,IAAA;AACA,OAFA;AAGA;AA7IA,GAzCA;AAwLA,EAAA,KAAA,EAAA;AACA,IAAA,IADA,kBACA;AACA,WAAA,OAAA,GAAA,CAAA;AACA,WAAA,YAAA;AACA,KAJA;AAKA,IAAA,UALA,wBAKA;AACA,WAAA,OAAA,GAAA,CAAA;AACA,WAAA,YAAA;AACA,KARA;AASA,IAAA,KATA,mBASA;AACA,WAAA,OAAA,GAAA,CAAA;AACA,WAAA,YAAA;AACA,KAZA;AAaA,IAAA,MAbA,oBAaA;AACA,WAAA,OAAA,GAAA,CAAA;AACA,WAAA,YAAA;AACA,KAhBA;AAiBA,IAAA,QAjBA,sBAiBA;AACA,WAAA,OAAA,GAAA,CAAA;AACA,WAAA,YAAA;AACA;AApBA,GAxLA;AA8MA,EAAA,QAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,aAAA,UAAA,IAAA,EAAA;AACA,YAAA,OAAA,GAAA,EAAA;AACA,YAAA,IAAA,GAAA,EAAA;;AACA,gBAAA,IAAA;AACA,eAAA,CAAA;AACA,YAAA,OAAA,GAAA,QAAA;AACA,YAAA,IAAA,GAAA,IAAA;AACA;;AACA,eAAA,CAAA;AACA,YAAA,OAAA,GAAA,SAAA;AACA,YAAA,IAAA,GAAA,IAAA;AACA;;AACA,eAAA,CAAA;AACA,YAAA,OAAA,GAAA,SAAA;AACA,YAAA,IAAA,GAAA,IAAA;AACA;AAZA;;AAcA,eAAA;AACA,UAAA,OAAA,EAAA,OADA;AAEA,UAAA,IAAA,EAAA;AAFA,SAAA;AAIA,OArBA;AAsBA,KAxBA;AAyBA,IAAA,QAzBA,sBAyBA;AACA,aAAA,UAAA,MAAA,EAAA;AACA,eAAA,KAAA,YAAA,IAAA,MAAA,GAAA,eAAA,GAAA,QAAA;AACA,OAFA;AAGA;AA7BA;AA9MA,C","sourcesContent":["<template>\n  <el-card class=\"main-card\">\n    <div class=\"title\">{{ this.$route.name }}</div>\n    <!-- 文章状态 -->\n    <div class=\"article-status-menu\">\n      <span style=\"font-weight: bold;color: red\">状态:</span>\n      <span @click=\"changeStauts('all')\" :class=\"isActive('all')\">全部</span>\n      <span @click=\"changeStauts('public')\" :class=\"isActive('public')\">\n        公开\n      </span>\n      <span @click=\"changeStauts('secret')\" :class=\"isActive('secret')\">\n        私密\n      </span>\n      <span @click=\"changeStauts('draft')\" :class=\"isActive('draft')\">\n        草稿箱\n      </span>\n      <span @click=\"changeStauts('delete')\" :class=\"isActive('delete')\">\n        回收站\n      </span>\n    </div>\n    <!-- 表格操作 -->\n    <div class=\"operation-container\">\n      <el-button\n        v-if=\"isDelete == 0\"\n        type=\"danger\"\n        size=\"small\"\n        icon=\"el-icon-delete\"\n        :disabled=\"articleIdList.length == 0\"\n        @click=\"updateIsDelete = true\"\n      >\n        批量删除\n      </el-button>\n      <el-button\n        v-else\n        type=\"danger\"\n        size=\"small\"\n        icon=\"el-icon-delete\"\n        :disabled=\"articleIdList.length === 0\"\n        @click=\"remove = true\"\n      >\n        批量删除\n      </el-button>\n      <!-- 条件筛选 -->\n      <div style=\"margin-left:auto\">\n        <!-- 文章类型 -->\n        <el-select\n          clearable\n          v-model=\"type\"\n          placeholder=\"请选择文章类型\"\n          size=\"small\"\n          style=\"margin-right:1rem\"\n        >\n          <el-option\n            v-for=\"item in typeList\"\n            :key=\"item.value\"\n            :label=\"item.label\"\n            :value=\"item.value\"\n          />\n        </el-select>\n        <!-- 分类 -->\n        <el-select\n          clearable\n          size=\"small\"\n          v-model=\"categoryId\"\n          filterable\n          placeholder=\"请选择分类\"\n          style=\"margin-right:1rem\"\n        >\n          <el-option\n            v-for=\"item in categoryList\"\n            :key=\"item.id\"\n            :label=\"item.categoryName\"\n            :value=\"item.id\"\n          />\n        </el-select>\n        <!-- 标签 -->\n        <el-select\n          clearable\n          size=\"small\"\n          v-model=\"tagId\"\n          filterable\n          placeholder=\"请选择标签\"\n          style=\"margin-right:1rem\"\n        >\n          <el-option\n            v-for=\"item in tagList\"\n            :key=\"item.id\"\n            :label=\"item.tagName\"\n            :value=\"item.id\"\n          />\n        </el-select>\n        <!-- 文章名 -->\n        <el-input\n          clearable\n          v-model=\"keywords\"\n          prefix-icon=\"el-icon-search\"\n          size=\"small\"\n          placeholder=\"请输入文章名\"\n          style=\"width:200px\"\n          @keyup.enter.native=\"searchArticles\"\n        />\n        <el-button\n          type=\"primary\"\n          size=\"small\"\n          icon=\"el-icon-search\"\n          style=\"margin-left:1rem\"\n          @click=\"searchArticles\"\n        >\n          搜索\n        </el-button>\n      </div>\n    </div>\n    <!-- 表格展示 -->\n    <el-table\n      border\n      :data=\"articleList\"\n      @selection-change=\"selectionChange\"\n      v-loading=\"loading\"\n    >\n      <!-- 表格列 -->\n      <el-table-column type=\"selection\" width=\"55\" />\n      <!-- 文章修改时间 -->\n      <el-table-column\n        prop=\"articleCover\"\n        label=\"文章封面\"\n        width=\"180\"\n        align=\"center\"\n      >\n        <template slot-scope=\"scope\">\n          <el-image\n            class=\"article-cover\"\n            :src=\"\n              scope.row.articleCover\n                ? scope.row.articleCover\n                : 'https://www.static.talkxj.com/articles/c5cc2b2561bd0e3060a500198a4ad37d.png'\n            \"\n          />\n          <i\n            v-if=\"scope.row.status === 1\"\n            class=\"iconfont el-icon-mygongkai article-status-icon\"\n          />\n          <i\n            v-if=\"scope.row.status === 2\"\n            class=\"iconfont el-icon-mymima article-status-icon\"\n          />\n          <i\n            v-if=\"scope.row.status === 3\"\n            class=\"iconfont el-icon-mycaogaoxiang article-status-icon\"\n          />\n        </template>\n      </el-table-column>\n      <!-- 文章标题 -->\n      <el-table-column prop=\"articleTitle\" label=\"标题\" align=\"center\" />\n      <!-- 文章分类 -->\n      <el-table-column\n        prop=\"categoryName\"\n        label=\"分类\"\n        width=\"110\"\n        align=\"center\"\n      />\n      <!-- 文章标签 -->\n      <el-table-column\n        prop=\"tagDTOList\"\n        label=\"标签\"\n        width=\"170\"\n        align=\"center\"\n      >\n        <template slot-scope=\"scope\">\n          <el-tag\n            v-for=\"item of scope.row.tagDTOList\"\n            :key=\"item.tagId\"\n            style=\"margin-right:0.2rem;margin-top:0.2rem\"\n          >\n            {{ item.tagName }}\n          </el-tag>\n        </template>\n      </el-table-column>\n      <!-- 文章浏览量 -->\n      <el-table-column\n        prop=\"viewsCount\"\n        label=\"浏览量\"\n        width=\"70\"\n        align=\"center\"\n      >\n        <template slot-scope=\"scope\">\n          <span v-if=\"scope.row.viewsCount\">\n            {{ scope.row.viewsCount }}\n          </span>\n          <span v-else>0</span>\n        </template>\n      </el-table-column>\n      <!-- 文章点赞量 -->\n      <el-table-column\n        prop=\"likeCount\"\n        label=\"点赞量\"\n        width=\"70\"\n        align=\"center\"\n      >\n        <template slot-scope=\"scope\">\n          <span v-if=\"scope.row.likeCount\">\n            {{ scope.row.likeCount }}\n          </span>\n          <span v-else>0</span>\n        </template>\n      </el-table-column>\n      <!-- 文章类型 -->\n      <el-table-column prop=\"type\" label=\"类型\" width=\"80\" align=\"center\">\n        <template slot-scope=\"scope\">\n          <el-tag :type=\"articleType(scope.row.type).tagType\">\n            {{ articleType(scope.row.type).name }}\n          </el-tag>\n        </template>\n      </el-table-column>\n      <!-- 文章发表时间 -->\n      <el-table-column\n        prop=\"createTime\"\n        label=\"发表时间\"\n        width=\"130\"\n        align=\"center\"\n      >\n        <template slot-scope=\"scope\">\n          <i class=\"el-icon-time\" style=\"margin-right:5px\" />\n          {{ scope.row.createTime | date }}\n        </template>\n      </el-table-column>\n      <!-- 文章置顶 -->\n      <el-table-column prop=\"isTop\" label=\"置顶\" width=\"80\" align=\"center\">\n        <template slot-scope=\"scope\">\n          <el-switch\n            v-model=\"scope.row.isTop\"\n            active-color=\"#13ce66\"\n            inactive-color=\"#F4F4F5\"\n            :disabled=\"scope.row.isDelete === 1\"\n            :active-value=\"1\"\n            :inactive-value=\"0\"\n            @change=\"changeTop(scope.row)\"\n          />\n        </template>\n      </el-table-column>\n      <!-- 列操作 -->\n      <el-table-column label=\"操作\" align=\"center\" width=\"150\">\n        <template slot-scope=\"scope\">\n          <el-button\n            type=\"primary\"\n            size=\"mini\"\n            @click=\"editArticle(scope.row.id)\"\n            v-if=\"scope.row.isDelete === 0\"\n          >\n            编辑\n          </el-button>\n          <el-popconfirm\n            title=\"确定删除吗？\"\n            style=\"margin-left:10px\"\n            @confirm=\"updateArticleDelete(scope.row.id)\"\n            v-if=\"scope.row.isDelete === 0\"\n          >\n            <el-button size=\"mini\" type=\"danger\" slot=\"reference\">\n              删除\n            </el-button>\n          </el-popconfirm>\n          <el-popconfirm\n            title=\"确定恢复吗？\"\n            v-if=\"scope.row.isDelete === 1\"\n            @confirm=\"updateArticleDelete(scope.row.id)\"\n          >\n            <el-button size=\"mini\" type=\"success\" slot=\"reference\">\n              恢复\n            </el-button>\n          </el-popconfirm>\n          <el-popconfirm\n            style=\"margin-left:10px\"\n            v-if=\"scope.row.isDelete == 1\"\n            title=\"确定彻底删除吗？\"\n            @confirm=\"deleteArticles(scope.row.id)\"\n          >\n            <el-button size=\"mini\" type=\"danger\" slot=\"reference\">\n              删除\n            </el-button>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n    <!-- 分页 -->\n    <el-pagination\n      class=\"pagination-container\"\n      background\n      @size-change=\"sizeChange\"\n      @current-change=\"currentChange\"\n      :current-page=\"current\"\n      :page-size=\"size\"\n      :total=\"count\"\n      :page-sizes=\"[10,30,50,100]\"\n      layout=\"total, sizes, prev, pager, next, jumper\"\n    />\n    <!-- 批量逻辑删除对话框 -->\n    <el-dialog :visible.sync=\"updateIsDelete\" width=\"30%\" :close-on-click-modal=\"false\">\n      <div class=\"dialog-title-container\" slot=\"title\">\n        <i class=\"el-icon-warning\" style=\"color:#ff9900\" />提示\n      </div>\n      <div style=\"font-size:1rem\">是否删除选中项？</div>\n      <div slot=\"footer\">\n        <el-button @click=\"updateIsDelete = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"updateArticleDelete(null)\">\n          确 定\n        </el-button>\n      </div>\n    </el-dialog>\n    <!-- 批量彻底删除对话框 -->\n    <el-dialog :visible.sync=\"remove\" width=\"30%\" :close-on-click-modal=\"false\">\n      <div class=\"dialog-title-container\" slot=\"title\">\n        <i class=\"el-icon-warning\" style=\"color:#ff9900\" />提示\n      </div>\n      <div style=\"font-size:1rem\">是否彻底删除选中项？</div>\n      <div slot=\"footer\">\n        <el-button @click=\"remove = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"deleteArticles(null)\">\n          确 定\n        </el-button>\n      </div>\n    </el-dialog>\n  </el-card>\n</template>\n\n<script>\n  import {articleTop, getArticleList, logicDeleteArticles} from \"../../api/article\";\n\nexport default {\n  created() {\n    this.listArticles();\n    this.listCategories();\n    this.listTags();\n  },\n  data: function() {\n    return {\n      loading: true,\n      updateIsDelete: false,\n      remove: false,\n      typeList: [\n        {\n          value: 1,\n          label: \"原创\"\n        },\n        {\n          value: 2,\n          label: \"转载\"\n        },\n        {\n          value: 3,\n          label: \"翻译\"\n        }\n      ],\n      activeStatus: \"all\",\n      articleList: [],\n      articleIdList: [],\n      categoryList: [],\n      tagList: [],\n      keywords: null,\n      type: null,\n      categoryId: null,\n      tagId: null,\n      isDelete: 0,\n      status: null,\n      current: 1,\n      size: 10,\n      count: 0\n    };\n  },\n  methods: {\n    selectionChange(articleList) {\n      this.articleIdList = [];\n      articleList.forEach(item => {\n        this.articleIdList.push(item.id);\n      });\n    },\n    searchArticles() {\n      this.current = 1;\n      this.listArticles();\n    },\n    editArticle(id) {\n      this.$router.push({ path: \"/articles/\" + id });\n    },\n    updateArticleDelete(id) {\n      let param = {};\n      if (id != null) {\n        param.idList = [id];\n      } else {\n        param.idList = this.articleIdList;\n      }\n      param.isDelete = this.isDelete === 0 ? 1 : 0;\n      logicDeleteArticles(param).then(({ data }) => {\n        if (data.flag) {\n          this.$notify.success({\n            title: \"成功\",\n            message: data.message\n          });\n          this.listArticles();\n        } else {\n          this.$notify.error({\n            title: \"失败\",\n            message: data.message\n          });\n        }\n        this.updateIsDelete = false;\n      });\n    },\n    // 批量彻底删除\n    deleteArticles(id) {\n      var param = {};\n      if (id == null) {\n        param = { data: this.articleIdList };\n      } else {\n        param = { data: [id] };\n      }\n      this.axios.delete(\"/api/admin/articles\", param).then(({ data }) => {\n        if (data.flag) {\n          this.$notify.success({\n            title: \"成功\",\n            message: data.message\n          });\n          this.listArticles();\n        } else {\n          this.$notify.error({\n            title: \"失败\",\n            message: data.message\n          });\n        }\n        this.remove = false;\n      });\n    },\n    sizeChange(size) {\n      this.size = size;\n      this.listArticles();\n    },\n    currentChange(current) {\n      this.current = current;\n      this.listArticles();\n    },\n    changeStauts(status) {\n      switch (status) {\n        case \"all\":\n          this.isDelete = 0;\n          this.status = null;\n          break;\n        case \"public\":\n          this.isDelete = 0;\n          this.status = 1;\n          break;\n        case \"secret\":\n          this.isDelete = 0;\n          this.status = 2;\n          break;\n        case \"draft\":\n          this.isDelete = 0;\n          this.status = 3;\n          break;\n        case \"delete\":\n          this.isDelete = 1;\n          this.status = null;\n          break;\n      }\n      this.activeStatus = status;\n    },\n    // 置顶\n    changeTop(article) {\n      articleTop({ id: article.id, isTop: article.isTop }).then(({ data }) => {\n        console.log('data',data)\n        if (data.flag) {\n          this.$notify.success({\n            title: \"成功\",\n            message: \"置顶成功\"\n          });\n        } else {\n          this.$notify.error({\n            title: \"失败\",\n            message: data.message\n          });\n          article.isTop = article.isTop === 0 ? 1 : 0;\n        }\n        this.remove = false;\n      });\n    },\n    listArticles() {\n      const params = {\n        current: this.current,\n        size: this.size,\n        keywords: this.keywords,\n        categoryId: this.categoryId,\n        status: this.status,\n        tagId: this.tagId,\n        type: this.type,\n        isDelete: this.isDelete\n      };\n      getArticleList(params).then(({ data }) => {\n        this.articleList = data.data.recordList;\n        console.log(this.articleList)\n        this.count = data.data.count;\n        this.loading = false;\n      });\n    },\n    listCategories() {\n      this.axios.get(\"/api/admin/categories/search\").then(({ data }) => {\n        this.categoryList = data.data;\n      });\n    },\n    listTags() {\n      this.axios.get(\"/api/admin/tags/search\").then(({ data }) => {\n        this.tagList = data.data;\n      });\n    }\n  },\n  watch: {\n    type() {\n      this.current = 1;\n      this.listArticles();\n    },\n    categoryId() {\n      this.current = 1;\n      this.listArticles();\n    },\n    tagId() {\n      this.current = 1;\n      this.listArticles();\n    },\n    status() {\n      this.current = 1;\n      this.listArticles();\n    },\n    isDelete() {\n      this.current = 1;\n      this.listArticles();\n    }\n  },\n  computed: {\n    articleType() {\n      return function(type) {\n        var tagType = \"\";\n        var name = \"\";\n        switch (type) {\n          case 1:\n            tagType = \"danger\";\n            name = \"原创\";\n            break;\n          case 2:\n            tagType = \"success\";\n            name = \"转载\";\n            break;\n          case 3:\n            tagType = \"primary\";\n            name = \"翻译\";\n            break;\n        }\n        return {\n          tagType: tagType,\n          name: name\n        };\n      };\n    },\n    isActive() {\n      return function(status) {\n        return this.activeStatus == status ? \"active-status\" : \"status\";\n      };\n    }\n  }\n};\n</script>\n\n<style scoped>\n.operation-container {\n  margin-top: 1.5rem;\n}\n.article-status-menu {\n  font-size: 14px;\n  margin-top: 40px;\n  color: #999;\n}\n.article-status-menu span {\n  margin-right: 24px;\n}\n.status {\n  cursor: pointer;\n}\n.active-status {\n  cursor: pointer;\n  color: #333;\n  font-weight: bold;\n}\n.article-cover {\n  position: relative;\n  width: 100%;\n  height: 90px;\n  border-radius: 4px;\n}\n.article-cover::after {\n  content: \"\";\n  background: rgba(0, 0, 0, 0.3);\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  left: 0;\n  right: 0;\n}\n.article-status-icon {\n  color: #fff;\n  font-size: 1.5rem;\n  position: absolute;\n  right: 1rem;\n  bottom: 1.4rem;\n}\n</style>\n"],"sourceRoot":"src/views/article"}]}