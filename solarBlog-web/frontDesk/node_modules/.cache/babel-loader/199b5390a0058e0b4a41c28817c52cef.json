ainer\">\n      <v-row>\n        <v-col :md=\"6\" v-for=\"item of photoAlbumList\" :key=\"item.id\">\n          <div class=\"album-item\">\n            <v-img class=\"album-cover\" :src=\"item.albumCover\" />\n            <router-link :to=\"'/albums/' + item.id\" class=\"album-wrapper\">\n              <div class=\"album-name\">\n                <span style=\"color:red;\">相册名:</span>\n                {{ item.albumName }}\n              </div>\n              <div class=\"album-desc\">\n                <span style=\"color:#FF8000;\">相册描述：</span>\n                {{ item.albumDesc }}\n              </div>\n            </router-link>\n          </div>\n        </v-col>\n      </v-row>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport {getAlbums} from \"../../api/file\";\n\nexport default {\n  created() {\n    this.listPhotoAlbums();\n  },\n  data: function() {\n    return {\n      photoAlbumList: []\n    };\n  },\n  methods: {\n    listPhotoAlbums() {\n      getAlbums().then(res => {\n        this.photoAlbumList = res.data;\n      });\n    }\n  }\n};\n</script>\n\n<style scoped>\n.album-item {\n  overflow: hidden;\n  position: relative;\n  cursor: pointer;\n  background: #000;\n  border-radius: 0.5rem !important;\n}\n.album-cover {\n  position: relative;\n  max-width: none;\n  width: calc(100% + 1.25rem);\n  height: 250px;\n  opacity: 0.8;\n  transition: opacity 0.35s, transform 0.35s;\n  transform: translate3d(-10px, 0, 0);\n  object-fit: cover;\n}\n.album-wrapper {\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  left: 0;\n  right: 0;\n  padding: 1.8rem 2rem;\n  color: #fff !important;\n}\n.album-item:hover .album-cover {\n  transform: translate3d(0, 0, 0);\n  opacity: 0.4;\n}\n.album-item:hover .album-name:after {\n  transform: translate3d(0, 0, 0);\n}\n.album-item:hover .album-desc {\n  opacity: 1;\n  filter: none;\n  transform: translate3d(0, 0, 0);\n}\n.album-name {\n  font-weight: bold;\n  font-size: 1.25rem;\n  overflow: hidden;\n  padding: 0.7rem 0;\n