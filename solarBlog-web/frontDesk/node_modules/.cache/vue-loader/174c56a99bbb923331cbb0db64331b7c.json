cesContent":["<template>\n  <div>\n    <!-- ÈìæÊé•ÂàóË°® -->\n    <v-card class=\"blog-container\">\n      <div class=\"link-title mb-1\">\n        <v-icon color=\"blue\">mdi-link-variant</v-icon> Â§ß‰Ω¨ÈìæÊé•\n      </div>\n      <v-row class=\"link-container\">\n        <v-col\n          class=\"link-wrapper\"\n          md=\"4\"\n          cols=\"12\"\n          v-for=\"item of friendLinkList\"\n          :key=\"item.id\"\n        >\n          <a :href=\"item.linkAddress\" target=\"_blank\">\n            <v-avatar size=\"65\" class=\"link-avatar\">\n              <img :src=\"item.linkAvatar\" />\n            </v-avatar>\n            <div style=\"width:100%;z-index:10;\">\n              <div class=\"link-name\">{{ item.linkName }}</div>\n              <div class=\"link-intro\">{{ item.linkIntro }}</div>\n            </div>\n          </a>\n        </v-col>\n      </v-row>\n      <!-- ËØ¥Êòé -->\n      <div class=\"link-title mt-4 mb-4\">\n        <v-icon color=\"blue\">mdi-dots-horizontal-circle</v-icon> Ê∑ªÂä†ÂèãÈìæ\n      </div>\n      <blockquote>\n        <div>ÂêçÁß∞Ôºö{{ blogInfo.websiteConfig.websiteName }}</div>\n        <div>ÁÆÄ‰ªãÔºö{{ blogInfo.websiteConfig.websiteIntro }}</div>\n        <div>Â§¥ÂÉèÔºö{{ blogInfo.websiteConfig.websiteAvatar }}</div>\n      </blockquote>\n      <div class=\"mt-5 mb-5\">\n        ÈúÄË¶Å‰∫§Êç¢ÂèãÈìæÁöÑÂèØÂú®‰∏ãÊñπÁïôË®Äüíñ\n      </div>\n      <blockquote class=\"mb-10\">\n        ÂèãÈìæ‰ø°ÊÅØÂ±ïÁ§∫ÈúÄË¶ÅÔºå‰Ω†ÁöÑ‰ø°ÊÅØÊ†ºÂºèË¶ÅÂåÖÂê´ÔºöÂêçÁß∞„ÄÅ‰ªãÁªç„ÄÅÈìæÊé•„ÄÅÂ§¥ÂÉè\n      </blockquote>\n      <!-- ËØÑËÆ∫ -->\n      <Comment\n        :commentList=\"commentList\"\n        :count=\"count\"\n        @reloadComment=\"listComments\"\n      />\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport Comment from \"../../components/Comment\";\nimport {getLinks} from \"../../api/user\";\nimport {getComments} from \"../../api/article\";\nexport default {\n  components: {\n    Comment\n  },\n  created() {\n    this.listFriendLink();\n    this.listComments();\n  },\n  data: function() {\n    return {\n      friendLinkList: [],\n      commentList: [],\n      count: 0\n    };\n  },\n  methods: {\n    listFriendLink() {\n      getLinks().then(res => {\n        this.friendLinkList = res.data;\n      });\n    },\n    listComments() {\n        getComments({ current: 1 }).then(res => {\n          this.commentList = res.data.recordList;\n          this.count = res.data.count;\n        });\n    }\n  },\n  computed: {\n    blogInfo() {\n      return this.$store.state.blogInfo;\n    },\n  }\n};\n</script>\n\n<style scoped>\nblockquote {\n  line-height: 2;\n  margin: 0;\n  font-size: 15px;\n  border-left: 0.2rem solid #49b1f5;\n  padding: 10px 1rem !important;\n  background-color: #ecf7fe;\n  border-radius: 4px;\n  color: steelblue;\n}\n.link-title {\n  color: steelblue;\n  font-size: 21px;\n  font-weight: bold;\n  line-height: 2;\n}\n.link-container {\n  margin: 10px 10px 0;\n}\n.link-wrapper {\n  position: relative;\n  transition: all 0.3s;\n  border-radius: 8px;\n}\n.link-avatar {\n  margin-top: 5px;\n  margin-left: 10px;\n  transition: all 0.5s;\n}\n@media (max-width: 759px) {\n  .link-avatar {\n    margin-left: 30px;\n  }\n}\n.link-name {\n  text-align: center;\n  font-size: 1.25rem;\n  font-weight: bold;\n  z-index: 1000;\n  color: #b31d28;\n}\n\n.link-intro {\n  text-align: center;\n  padding: 16px 10px;\n  height: 50px;\n  font-size: 13px;\n  color: steelblue;\n  width: 100%;\n}\n.link-wrapper:hover a {\n  color: #fff;\n}\n.link-wrapper:hover .link-intro {\n  color: #fff;\n}\n.link-wrapper:hover .link-avatar {\n  transform: rotate(360deg);\n}\n.link-wrapper a {\n  color: #333;\n  text-decoration: none;\n  display: flex;\n  height: 100%;\n  width: 100%;\n}\n.link-wrapper:hover {\n  box-shadow: 0 2px 20px #49b1f5;\n}\n.link-wrapper:hover:before {\n  transform: scale(1);\n}\n.link-wrapper:before {\n  position: absolute;\n  border-radius: 8px;\n  top: 0;\n  right: 0;\n  bottom: 0;\n  left: 0;\n  background: #49b1f5 !important;\n  content: \"\";\n  transition-timing-function: ease-out;\n  transition-duration: 0.3s;\n  transition-property: transform;\n  transform: scale(0);\n}\n</style>\n"]}]}